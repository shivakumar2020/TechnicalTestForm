properties([pipelineTriggers([githubPush()])])

node {
        git url: 'https://github.com/shivakumar2020/TechnicalTestForm.git',branch: 'main'
        stage ('Compile Stage') {

            echo "compiling"
             sh 'mvn clean compile'
            echo "compilation completed"
            
        }

        stage ('Testing Stage') {

            echo "testing"
             sh 'mvn test'
            echo "testing completed"
            
        }
        
            
}